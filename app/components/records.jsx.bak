import React from 'react';
import {render} from 'react-dom';
import { Segment } from 'semantic-ui-react';

export default class BK_Records extends React.Component {
  constructor(props) {
    super(props);
  }

  render() {
    const { records } = this.props;
    console.log(records);

    return (
      <div>
        { records.map(function(record, i) {
            return (
              <div>
                <p>{record.provider}</p>
                <p>{record.content}</p>
              </div>
            );
          })}
      </div>
    );
  }
}
